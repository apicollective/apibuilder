  "host": "petstore.swagger.wordnik.com",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "paths": {
    "/pets/{status}": {
      "get": {
        "summary": "find pets by status - as a path param",
        "parameters":[
          {
            "name":"status",
            "in":"path",
            "required":true,
            "type":"string",
            "enum":["available", "pending", "sold"]
          }
        ],
        "responses": {
          "200": {
            "description": "find pet response",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Pet"
              }
            },
            "headers": {
              "x-expires": {
                "type": "string"
              }
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "delete": {
        "summary": "delete pets by status - as a path param",
        "operationId": "deletePet",
        "tags": ["tag1", "tag2"],
        "parameters":[
          {
            "name":"status",
            "in":"path",
            "required":true,
            "type":"string",
            "enum":["available", "pending", "sold"]
          }
        ],
        "responses": {
          "204": {
            "description": "pets deleted - no response body content"
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/pets/": {
      "get": {
        "summary": "find pets by name and status - as query params",
        "parameters":[
          {
            "name":"name",
            "in":"query",
            "required":true,
            "type":"string"
          },
          {
            "name":"status",
            "in":"query",
            "required":true,
            "type":"string",
            "enum":["available", "pending", "sold"]
          }
        ],
        "responses": {
          "200": {
            "description": "find pet response",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Pet"
              }
            },
            "headers": {
              "x-expires": {
                "type": "string"
              }
            },
            "x-foo-response": {
              "bar": "response"
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        },
        "x-foo-operation": {
          "bar": "operation"
        }
      }
    }
  },
  "definitions": {
    "Pet": {
      "description": "Definition of a pet",
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "email_address": {
          "type": "string",
          "format": "email"
        },
        "tag": {
          "type": "string",
          "x-foo-field": {
            "bar": "field"
          }
        },
        "additional_info": {
          "type": "object"
        },
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "status": {
          "type": "string",
          "enum": ["available", "pending", "sold"]
        },
        "html_description": {
          "type": "string",
          "format": "html"
        },
        "siblings": {
          "items": {
            "$ref": "#/definitions/Pet"
          },
          "type": "array"
        }
      },
      "x-foo-model": {
        "bar": "model"
      }
    },
    "Error": {
      "required": [
        "code",
        "message"
      ],
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        }
      }
    }
  },
  "x-foo-service": {
    "bar": "service"
  }
