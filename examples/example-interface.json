{
  "name": "example-interface",

  "interfaces": {
    "database_record": {
      "fields": [
        { "name": "id", "type": "string" }
      ]
    },
    "person": {
      "fields": [
        { "name": "email", "type": "string", "required": false },
        { "name": "name", "type": "string", "required": false }
      ]
    }
  },

  "models": {
    "user": {
      "interfaces": ["database_record", "person"],
      "fields": [
        { "name": "registration_date", "type": "date-time-iso8601" }
      ]
    },

    "guest": {
      "interfaces": ["person"],
      "fields": [
        { "name": "seession_id", "type": "string" }
      ]
    }
  }
}

  
